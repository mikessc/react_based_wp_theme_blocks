{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "tbwa-blocks/legal-snippet",
	"version": "1.0",
	"title": "Legal Snippet Container",
	"category": "tbwa-blocks",
	"description": "Container for custom legal snippets.",
	"supports": {
		"html":false
	},
	"parent": ["core/post-content", "tbwa-blocks/page"],
	"attributes": {
		"background": {
			"type": "string", 
			"default": "white"
		},
		"stripe": {
			"type": "string", 
			"default": "none"
		},
		"width": {
			"type": "string", 
			"default": "standard"
		}
	},
	"textdomain": "tbwa-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./frontend.js" 
}